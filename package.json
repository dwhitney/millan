{
  "dependencies": {
    "chevrotain": "3.0.1"
  },
  "name": "millan",
  "description": "The (Star)dog Whisperer (node.js-based parsers for Stardog)",
  "version": "1.0.0",
  "main": "dist/index.js",
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "rollup": "^0.58.1",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(test|spec)\\.[tj]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "scripts": {
    "precommit": "node ./build/scripts/precommit.js; ./node_modules/rollup/bin/rollup -c; git add ./dist;",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stardog-union/millan.git"
  },
